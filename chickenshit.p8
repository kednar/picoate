pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
function _init()
pl = {}
pl.x = 64
pl.y = 110
pl.dx = 0
pl.dy = 0
pl.ddy = 0.06
pl.jump = 0
pl.jspeed = 15
pl.fall = 0
pl.lock = 0
pl.shot = 0
pl.sprite = 0
aim = {}
aim.x = 0
aim.y = 0
bt = 1 -- bullet time
end

function _update()

--gravity	
if (pl.y < 110) and (pl.lock == 0) then
		if (pl.dy == 0) then
			pl.dy = pl.dy + pl.ddy
			pl.sprite = 4
			end
	end

-- engage lock
if (pl.lock == 0) and (btnp(4)) then
	pl.lock = 1
	tlock = 0
	bt = 0.5 -- bullet time
	end

-- locked mode aiming
if pl.lock == 1 then
	pl.sprite = 5
	aim.x = pl.x
	aim.y = pl.y
	if (btn(0)) then aim.x = (pl.x-10) end
	if (btn(1)) then aim.x = (pl.x+10) end
	if (btn(2)) then aim.y = (pl.y-10) end
	if (btn(3)) then aim.y = (pl.y+10) end		
	tlock = tlock + 1
	if (tlock == 90) then
		pl.lock = 0
		bt = 1
		end
	end	
	
-- movement left / right
if (pl.lock == 0) then
	if (btn(0)) then 
		pl.x=pl.x-2
		pl.sprite = 2
		end
	if (btn(1)) then
 	pl.x=pl.x+2
 	pl.sprite = 1
		end
	end
 
-- jumping 
if (pl.jump == 0) and (pl.lock == 0) and (btnp(2)) then
 pl.jump = 1
 pl.dy = -pl.jspeed
	end
	 
if (pl.jump == 1) and (pl.lock == 0) then
 pl.dy = pl.dy+1
	pl.sprite = 3
	end
	
-- if (pl.dy == 0) then
--	pl.jump = 0
--	end
	
-- gravity and friction
if pl.lock == 0 then
	pl.dy = pl.dy + pl.ddy
	pl.dy = pl.dy * 0.9
	end

-- vertical movement
if pl.lock == 0 then
	pl.y = pl.y+pl.dy
	end

-- calibrate on bottom, reset jump
if (pl.y > 110) then pl.y = 110 end
if (pl.y == 110) then
	pl.fall = 0
	pl.jump = 0
	end

-- enemy
-- if fr.count == 120 then

end -- end update ()

function _draw()

-- background
 rectfill(0,0,127,127,5)
 
-- player
 spr(pl.sprite,pl.x,pl.y)
 
-- locked aim line
if pl.lock == 1 then
	 line(pl.x+4, pl.y+4, aim.x+4, aim.y+4, 4)
  line(pl.x-1, pl.y+10, (pl.x+flr(tlock/9)), pl.y+10, 11)
	for i=1,10,1 do
		aim.xx = aim.x
		aim.yy = aim.y
		aim.x = i*(aim.x/2)
		aim.y = i*(aim.y/2)
		line(aim.xx, aim.yy, aim.x, aim.y, 8)
		end
 end
 
 
 -- reset player sprite
 pl.sprite = 0
 
end -- end draw
